{
  "name": "begin-app",
  "version": "0.0.0",
  "description": "Begin basic Hello World! app for Deno",
  "scripts": {
    "start": "run-s cross-env",
    "linty": "eslint src --ignore-pattern node_modules --fix",
    "cross-env": "cross-env NODE_ENV=testing sandbox",
    "deno:install": "deno cache --unstable src/deps.ts",
    "deno:run":"deno run --allow-env --unstable --allow-read src/deps.ts"

  },
  "devDependencies": {
    "@architect/sandbox": "latest",
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "axios": "0.21.1",
    "lodash": "4.17.21",
    "npm-run-all": "^4.1.5",
    "sbd": "1.0.18"
  }
}
